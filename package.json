{
  "name": "alkomaholi",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "8.x.x",
    "yarn": "1.2.1"
  },
  "scripts": {
    "testprod": "NODE_ENV=production MONGO_URL=mongodb://localhost:27017 npm start",
    "dev": "MONGO_URL=mongodb://localhost:27017 nodemon --exec npm start",
    "lint": "eslint .",
    "start": "node ./server/app.js",
    "build-client": "cd client && npm install && npm run build",
    "test": "MONGO_URL=mongodb://localhost:27017 jest",
    "test-watch": "MONGO_URL=mongodb://localhost:27017 ENVIRONMENT=local jest --watch"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "cors": "^2.8.4",
    "es6-promise": "^4.2.2",
    "express": "~4.19.2",
    "file-type": "^7.4.0",
    "http-proxy-middleware": "^0.17.4",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.20.1",
    "mongoose": "^5.0.8",
    "node-fetch": "^1.7.3",
    "serve-favicon": "~2.4.5",
    "underscore": "^1.8.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.6.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.0.6",
    "nodemon": "^1.14.11"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/client/"
    ]
  },
  "author": "Hunttis",
  "license": "Apache-2.0"
}
